{
  "require": {
    "gamajo/template-loader": "^1.3",
    "brain/cortex": "~1.0.0",
    "htmlburger/carbon-fields": "^2.2",
    "alessandrotesoro/wp-notices": "^1.0",
    "stevegrunwell/wp-cache-remember": "^1.1",
    "pressmodo/carbon-fields-custom-options-container": "^0.1.0",
    "rarst/wpdatetime": "dev-master",
    "posterno/posterno-schema": "@dev",
    "posterno/posterno-third-party": "@dev",
    "posterno/posterno-maps": "@dev",
    "posterno/posterno-geocoder": "@dev",
    "posterno/posterno-forms": "@dev",
    "posterno/posterno-validator": "@dev"
  },
  "minimum-stability": "dev",
  "repositories": [{
      "type": "path",
      "url": "../posterno-schema",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../posterno-geocoder",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../posterno-third-party",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../posterno-maps",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../posterno-forms",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../posterno-validator",
      "options": {
        "symlink": true
      }
    }
  ],
  "extra": {
    "installer-paths": {
      "includes/utils/{$name}/": [
        "stevegrunwell/wp-cache-remember"
      ],
      "includes/components/{$name}/": [
        "posterno/posterno-schema",
        "posterno/posterno-geocoder",
        "posterno/posterno-third-party",
        "posterno/posterno-maps",
        "posterno/posterno-forms",
        "posterno/posterno-validator"
      ]
    }
  }
}
